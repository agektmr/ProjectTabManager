{
  "name": "ProjectTabManager",
  "version": "4.0.0",
  "description": "Project tab management per window on your Chrome",
  "main": "app/js/background.js",
  "scripts": {
    "postinstall": "bower install",
    "vulcanize": "vulcanize src/elements/elements.html | crisper -h src/elements/vulcanized.elements.html -j src/elements/vulcanized.elements.js",
    "build": "npm run clean; npm run vulcanize; grunt vulcanizeless --force",
    "clean": "rm -rf app/*"
  },
  "repository": "",
  "keywords": [
    "ProjectTabManager"
  ],
  "author": "Eiji Kitamura <agektmr+github@gmail.com>",
  "license": "Apache2.0",
  "devDependencies": {
    "concat-files": "^0.1.0",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-compress": "^0.13.0",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-sass": "^0.8.1",
    "grunt-markdown": "^0.6.1",
    "grunt-replace": "^0.9.2",
    "grunt-version": "^0.3.0",
    "grunt-vulcanize": "^0.6.1",
    "load-grunt-tasks": "^2.0.0"
  }
}
